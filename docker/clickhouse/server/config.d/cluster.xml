<clickhouse replace="true">
    <default_replica_path>/clickhouse/{cluster}/tables/{shard}/{database}/{table}</default_replica_path>
    <default_replica_name>{replica}</default_replica_name>
    <remote_servers>
        <scratch>
            <secret>$SECRET</secret>
            <shard>
                <weight>1</weight>
                <internal_replication>false</internal_replication>
                <replica>
                    <priority>1</priority>
                    <host>ch_one</host>
                    <port>9000</port>
                </replica>
                <replica>
                    <priority>2</priority>
                    <host>ch_two</host>
                    <port>9000</port>
                </replica>
            </shard>
            <shard>
                <weight>1</weight>
                <internal_replication>false</internal_replication>
                <replica>
                    <priority>1</priority>
                    <host>ch_three</host>
                    <port>9000</port>
                </replica>
            </shard>
        </scratch>
    </remote_servers>
</clickhouse>
