                <internal_replication>false</internal_replication>
                <replica>
                    <priority>1</priority>
                    <host>ch_one</host>
                    <port>9000</port>
                </replica>
                <replica>
                    <priority>2</priority>
                    <host>ch_two</host>
                    <port>9000</port>
                </replica>
            </shard>
            <shard>
                <weight>1</weight>
                <internal_replication>false</internal_replication>
                <replica>
                    <priority>1</priority>
                    <host>ch_new</host>
                    <port>9000</port>
                </replica>
            </shard>
        </local_cluster>
    </remote_servers>
</clickhouse>
